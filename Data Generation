        "  data = getData(i)\n",
        "  perfectData.append(data[0])\n",
        "  twentyFivePercentData.append(data[1][2])\n",
        "  fiftyPercentData.append(data[1][1])\n",
        "  seventyFivePercentData.append(data[1][0])"
